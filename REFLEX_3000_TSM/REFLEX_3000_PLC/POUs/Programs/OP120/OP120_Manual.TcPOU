	IF I120_CONV_REVI600_7 THEN
		SR120_Oring_Feed_Manual_Axis.targetVelocity :=-50.0;
		SR120_Oring_Feed_Manual_Axis.moveVelocity	:=I120_CONV_REVI600_7;
 	ELSIF I120_CONV_FWDI601_0 THEN
