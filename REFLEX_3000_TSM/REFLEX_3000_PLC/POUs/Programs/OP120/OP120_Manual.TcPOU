	BlinkingBit:= , 
	TimeOut1:=5 , 
	TimeOut2:=5 , 
	TimeOut3:=5 , 
	Solenoid1=>O120_LH_SING_CYL_REV_Q1261_4 , 
	Solenoid2=>O120_LH_SING_CYL_FWD_Q1261_5 , 
	AlarmSensor1=>berror[1119] , 
	AlarmSensor2=>berror[1120] , 
	AlarmSolenoid1=>berror[1121] , 
	AlarmSolenoid2=>berror[1122] , 
