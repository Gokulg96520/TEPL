  <POU Name="OP10_90_MODE_TIMER_BLCK" Id="{c1f76e31-1bb9-42b1-a1fd-98d848bbe404}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM OP10_90_MODE_TIMER_BLCK
VAR
	


	TEMP_MANUAL: BOOL;
	ERROR: BOOL;
	TEMP_AUTO: BOOL;
	MODE_BLCK: Mode_Selection_Block1;
	MODEWORD: INT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF TEMP_MANUAL AND NOT ERROR THEN
MODEWORD		:=2;
ELSE
MODEWORD		:=0;	
END_IF

IF TEMP_AUTO AND NOT ERROR THEN
MODEWORD		:=1;
	ELSE
MODEWORD		:=0;
END_IF


