	ModeWord:= MODEWORD, 
	HomeConditionOK:=1, 
	OperationEnable:= , 
	StartPB:= , 
	SafetyAndNOError:= ERROR, 
	ModeAck:= , 
	HomingINProgress:= , 
	TempHomeOK:= , 
	ResetPB:= , 
	AutoSelected=> , 
	ManualSelected=> , 
	StepSelected=> , 
	ModeSelAck_Indication=> , 
	StatusDisplay=> , 
	AutoRunning:= , 
	StepRunning:= , 
	StartEnable:= , 
	MachineStop:= );]]></ST>
    </Implementation>
